code = "// NOTE: Shader automatically converted from Godot Engine 4.3.beta's StandardMaterial3D.

shader_type spatial;
render_mode blend_mix, depth_draw_opaque, cull_back, diffuse_burley, specular_schlick_ggx, unshaded;

varying float depth;
void vertex() {
	depth = (MODEL_MATRIX*vec4(VERTEX, 1.0)).y;
}

void fragment() {
	ALBEDO = COLOR.rgb;
	if (depth > 0.01) {
		ALBEDO.r = 1.0;
		if (depth < 0.04) {
			ALBEDO.g = 1.0;
			ALBEDO.b = 1.0;
		}
	} else {
		float darkfac = pow(max(0.0, depth + 0.86), 4.0);
		ALBEDO = ALBEDO*darkfac;
		ALPHA = darkfac;
	}
}
"

