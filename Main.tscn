code = "shader_type spatial;
render_mode blend_mix, depth_draw_opaque, cull_back, diffuse_burley, specular_schlick_ggx;


uniform float multscale = 2.0;
uniform sampler2D voice;

void vertex() {
}

void fragment() {
  ALBEDO = vec3(UV, 0.5);
  vec4 v = texture(voice, UV);
  float s = v.r*multscale;
  float d = UV.y*2.0 - s*0.5 - 1.0;
  //EMISSION = (abs(d) < 0.1 ? vec3(0.8,0.8,0.9) : vec3(0.1,0.1,0.1));
  if (abs(UV.y - 0.5) < abs(s)*0.5)
	EMISSION = vec3(max(s, 0.0), 0.0, max(-s, 0.0));
}
"

